{
  "name": "airbrake/airbrake-wordpress",
  "description": "Airbrake exception and error notifier for PHP",
  "keywords": [
    "airbrake",
    "notifier",
    "exception",
    "error",
    "logging",
    "wordpress"
  ],
  "homepage": "https://airbrake.io",
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "airbrake/phpbrake": "^0.6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Airbrake\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
